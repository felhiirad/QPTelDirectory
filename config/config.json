{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "qp-telephone-directory-app-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/qpTelephoneDirectoryApp/QpTelephoneDirectoryAppWebPart.js",
          "manifest": "./src/webparts/qpTelephoneDirectoryApp/QpTelephoneDirectoryAppWebPart.manifest.json"
        }
      ]
    },
    "telephone-directory-search-zone-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/telephoneDirectorySearchZone/TelephoneDirectorySearchZoneWebPart.js",
          "manifest": "./src/webparts/telephoneDirectorySearchZone/TelephoneDirectorySearchZoneWebPart.manifest.json"
        }
      ]
    },
    "global-search-extension-application-customizer": {
      "components": [
        {
          "entrypoint": "./lib/extensions/globalSearchExtension/GlobalSearchExtensionApplicationCustomizer.js",
          "manifest": "./src/extensions/globalSearchExtension/GlobalSearchExtensionApplicationCustomizer.manifest.json"
        }
      ]
    },
    "qp-telephone-directory-details-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/qpTelephoneDirectoryDetails/QpTelephoneDirectoryDetailsWebPart.js",
          "manifest": "./src/webparts/qpTelephoneDirectoryDetails/QpTelephoneDirectoryDetailsWebPart.manifest.json"
        }
      ]
    }
  },
  "externals": {},
  "localizedResources": {
    "QpTelephoneDirectoryAppWebPartStrings": "lib/webparts/qpTelephoneDirectoryApp/loc/{locale}.js",
    "TelephoneDirectorySearchZoneWebPartStrings": "lib/webparts/telephoneDirectorySearchZone/loc/{locale}.js",
    "GlobalSearchExtensionApplicationCustomizerStrings": "lib/extensions/globalSearchExtension/loc/{locale}.js",
    "QpTelephoneDirectoryDetailsWebPartStrings": "lib/webparts/qpTelephoneDirectoryDetails/loc/{locale}.js"
  }
}
